        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Coach)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Gym)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_DayOfWeek)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Time_From)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Duration)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
