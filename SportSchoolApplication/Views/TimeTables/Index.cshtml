@model IEnumerable<SportSchoolApplication.Models.TimeTable>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id_Coach)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Id_Gym)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Id_DayOfWeek)
        </th>
